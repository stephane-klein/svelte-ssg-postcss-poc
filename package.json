{
  "name": "my-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p dev:postcss dev:vite",
    "dev:vite": "vite dev",
    "dev:postcss": "postcss src/yellow.css -w -m -o static/yellow.css",
    "build": "run-s build:postcss build:vite",
    "build:vite": "vite build",
    "build:postcss": "postcss src/yellow.css -o static/yellow.css",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.67",
    "@sveltejs/adapter-static": "1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.445",
    "autoprefixer": "10.4.8",
    "postcss": "8.4.16",
    "postcss-cli": "10.0.0",
    "postcss-import": "14.1.0",
    "postcss-load-config": "4.0.1",
    "postcss-nested": "5.0.6",
    "postcss-normalize": "10.0.1",
    "svelte": "3.49.0",
    "svelte-check": "2.8.1",
    "svelte-preprocess": "4.10.7",
    "vite": "3.0.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "tslib": "2.4.0",
    "typescript": "4.8.2"
  },
  "peerDependencies": {
    "browserslist": ">=4.0.0"
  },
  "type": "module"
}
